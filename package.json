{
  "name": "llmconfig",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.25.0",
  "description": "基于@ai-sdk,允许封装多个llm provider.使得配置与调用分离。",
  "author": "lizhutang",
  "license": "MIT",
  "funding": "https://github.com/sponsors/masol",
  "homepage": "https://github.com/masol/llmconfig#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masol/llmconfig.git"
  },
  "bugs": "https://github.com/masol/llmconfig/issues",
  "keywords": [
    "llm",
    "config"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "catalog:",
    "@ai-sdk/google": "catalog:",
    "@ai-sdk/openai": "catalog:",
    "@ai-sdk/xai": "catalog:",
    "ai": "catalog:",
    "html-entities": "catalog:",
    "json5": "catalog:",
    "lodash": "catalog:",
    "marked": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:cli",
    "@antfu/ni": "catalog:cli",
    "@antfu/utils": "catalog:inlined",
    "@types/lodash": "catalog:",
    "@types/node": "catalog:types",
    "bumpp": "catalog:cli",
    "dexie": "catalog:",
    "eslint": "catalog:cli",
    "lint-staged": "catalog:cli",
    "publint": "catalog:cli",
    "simple-git-hooks": "catalog:cli",
    "tinyexec": "catalog:testing",
    "tsdown": "catalog:",
    "tsx": "catalog:cli",
    "typescript": "catalog:cli",
    "vite": "catalog:",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
